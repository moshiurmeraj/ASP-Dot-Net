@using PartialViewPassingModel.Models
@model IEnumerable<PartialViewPassingModel.Models.Product>
@{
    ViewBag.Title = "Home Page";
}
@section featured {
    <section class="featured">
        <div class="content-wrapper">
            <hgroup class="title">
                <h1>@ViewBag.Title.</h1>
                <h2>@ViewBag.Message</h2>
            </hgroup>
        </div>
    </section>
}

@{
    List<Product> productsList = new List<Product>(Model);
}

<p>
    Partial View
    @Html.Partial("~/Views/Shared/_PartialView1.cshtml", productsList)

</p>



